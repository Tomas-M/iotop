#!/bin/sh

set -e

# generated by dh_installalternatives/13.1
if [ "$1" = "configure" ] || [ "$1" = "abort-upgrade" ] || [ "$1" = "abort-deconfigure" ] || [ "$1" = "abort-remove" ]; then
        update-alternatives --install /usr/sbin/iotop iotop /usr/sbin/iotop-c 40 \
            --slave /usr/share/man/man8/iotop.8.gz iotop.8.gz /usr/share/man/man8/iotop-c.8.gz
fi

#DEBHELPER#

exit 0
